<?xml version="1.0"?>
<odoo>

    <!-- Lead Menu - 2022-->
    <record model="ir.actions.act_window" id="crm_lead_2022">
        <field name="name">Year 2022 Leads</field>
        <field name="res_model">crm.lead</field>
        <field name="view_mode">tree,kanban,graph,pivot,calendar,form,activity</field>
        <field name="view_id" ref="crm.crm_case_tree_view_leads"/>
        <field name="domain">[('create_date', '&lt;=', '2022-12-31 11:59:59'),('type', '=', 'lead')
        ]</field>
        <field name="search_view_id" ref="crm.view_crm_case_leads_filter"/>
        <field name="context">{
            'default_type':'lead',
            'search_default_type': 'lead',
            'search_default_to_process':1,
            }
        </field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a Lead
            </p>
            <p>
                Leads are the qualification step before the creation of an opportunity.
            </p>
        </field>
    </record>

